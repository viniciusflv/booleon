{
  "name": "booleon",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "setup": "lerna bootstrap",
    "lint": "lint-staged",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:watch": "jest --watch",
    "dev": "start-storybook -p 6066",
    "build": "build-storybook -c .storybook -o build/dist",
    "format": "prettier --write $PWD/**/*.{ts,tsx}",
    "pub": "npm test && npm run bun && lerna publish",
    "bun": "run-p bun:*",
    "bun:core": "lerna exec --scope=@booleon/core -- npm run build",
    "bun:modules": "lerna exec --scope=@booleon/modules -- npm run build",
    "bun:react": "lerna exec --scope=@booleon/react -- npm run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm test --bail"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.11.1",
    "**/@typescript-eslint/parser": "^4.11.1",
    "**/jest": "^26.6.3",
    "**/ts-jest": "^26.4.4",
    "**/typescript": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@booleon/core": "^1.1.0-alpha.0",
    "@booleon/modules": "^1.1.1-alpha.0",
    "@booleon/react": "^1.1.1-alpha.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@storybook/addon-a11y": "^6.0.28",
    "@storybook/addon-backgrounds": "^6.0.28",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.0.28",
    "@storybook/addon-storysource": "^6.0.28",
    "@storybook/addons": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@types/jest-axe": "^3.5.1",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "commitizen": "^4.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "jest-axe": "^4.1.0",
    "lerna": "^3.22.1",
    "lint-staged": "^10.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "prettier-eslint-cli": "^5.0.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "ts-loader": "^8.0.11",
    "tsdx": "^0.14.1",
    "typescript": "^4.1.3"
  }
}
