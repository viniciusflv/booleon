{
  "name": "@booleon/core",
  "version": "2.0.4-alpha.0",
  "description": "Booleon core css compiler functions",
  "author": "Vin√≠cius Victorino",
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "module": "dist/core.esm.js",
  "devDependencies": {
    "typescript": "^4.2.4",
    "tsdx": "^0.13.2"
  },
  "scripts": {
    "size": "size-limit",
    "emit": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "build": "tsdx build --transpileOnly && npm run emit"
  },
  "size-limit": [
    {
      "path": "dist/core.cjs.production.min.js",
      "limit": "3 KB"
    },
    {
      "path": "dist/core.esm.js",
      "limit": "3 KB"
    }
  ],
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/viniciusflv/booleon.git"
  }
}
