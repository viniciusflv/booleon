{
  "name": "@booleon/docs",
  "version": "1.0.0",
  "private": true,
  "description": "Booleon Docs",
  "author": "Vinicius Victorino",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "svg": "npx svj -i $(pwd)/src/assets/svg -d $(pwd)/src/assets --ts --svgo",
    "dev": "npm run clean && npm run start",
    "prd": "npm run clean && npm run build && npm run serve",
    "deploy": "npm run build && gh-pages -d public",
    "start": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "build": "gatsby build --prefix-paths",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@booleon/modules": "file:../modules/src",
    "@booleon/react": "file:../react/src",
    "@booleon/ui": "file:../ui/src",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "cross-env": "^7.0.3",
    "gatsby": "^3.14.5",
    "gatsby-plugin-image": "^1.3.0",
    "gatsby-plugin-manifest": "^3.3.0",
    "gatsby-plugin-mdx": "^2.3.0",
    "gatsby-plugin-offline": "^4.14.0",
    "gatsby-plugin-react-helmet": "^4.3.0",
    "gatsby-plugin-sharp": "^3.3.0",
    "gatsby-plugin-sitemap": "^3.3.0",
    "gatsby-remark-mermaid": "^2.1.0",
    "gatsby-remark-prismjs": "^5.11.0",
    "gatsby-source-filesystem": "^3.3.0",
    "gatsby-transformer-sharp": "^3.3.0",
    "react-helmet": "^6.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
